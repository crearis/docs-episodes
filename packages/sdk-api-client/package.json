{
  "name": "@erpgap/odoo-sdk-api-client",
  "version": "0.1.0",
  "private": false,
  "sideEffects": false,
  "server": "server/index.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "license": "VSFEL",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "build": "rimraf lib server && rollup -c",
